{
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "extensions": [
        "ms-python.python",
        "snooty.snooty",
        "orta.vscode-jest"
    ],
    "postStartCommand": "cd client && npm install && cd -; poetry install",
    "remoteUser": "vscode",
    "forwardPorts": [
        3000,
        8000
    ]
}